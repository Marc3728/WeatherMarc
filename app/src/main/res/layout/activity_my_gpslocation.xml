<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.MyGPSLocation">

    <TextView
        android:id="@+id/infoubimia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/aplicarubiactual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CREAR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/infoubimia"
        app:layout_constraintStart_toStartOf="@+id/infoubimia"
        app:layout_constraintTop_toBottomOf="@+id/infoubimia" />

    <Button
        android:id="@+id/cancelarmiubiac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="CANCELAR"
        app:layout_constraintEnd_toEndOf="@+id/aplicarubiactual"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toStartOf="@+id/aplicarubiactual"
        app:layout_constraintTop_toBottomOf="@+id/aplicarubiactual" />
</androidx.constraintlayout.widget.ConstraintLayout>